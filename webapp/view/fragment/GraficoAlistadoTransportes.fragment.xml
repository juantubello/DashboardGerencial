<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns:table="sap.ui.table" xmlns="sap.m" xmlns:viz="sap.viz.ui5.controls"
	xmlns:layout="sap.ui.layout" xmlns:mvc="sap.ui.core.mvc" xmlns:viz.feeds="sap.viz.ui5.controls.common.feeds"
	xmlns:viz.data="sap.viz.ui5.data">
	<viz:VizFrame id="vizFrameAlistadoTransportes" uiConfig="{applicationSet:'fiori'}" width='100%' height='280px' vizType='donut'
		selectData="selectColumnData">
		<viz:dataset>
			<viz.data:FlattenedDataset data="{/rowsAlistadoTransportes}">
				<viz.data:dimensions>
					<viz.data:DimensionDefinition name="Estado" value="{Descripcion}"/>
				</viz.data:dimensions>
				<viz.data:measures>
					<viz.data:MeasureDefinition name="Transporte" value="{Cantidad}"/>
				</viz.data:measures>
			</viz.data:FlattenedDataset>
		</viz:dataset>
		<viz:feeds>
			<viz.feeds:FeedItem uid="size" type="Measure" values="Transporte"/>
			<viz.feeds:FeedItem uid="color" type="Dimension" values="Estado"/>
		</viz:feeds>
	</viz:VizFrame>
</core:FragmentDefinition>